{
    "name": "chaodada/monolog-timezone",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "logger",
        "timezone"
    ],
    "description": "解决Hyperf框架Log类出现时区的问题",
    "autoload": {
        "psr-4": {
            "Iuu\\MonologTimeZone\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "hyperf/logger": "~3.1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "Iuu\\MonologTimeZone\\ConfigProvider"
        }
    }
}
